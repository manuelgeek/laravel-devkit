{
    "name": "appslabke/laravel-devkit",
    "description": "This is a laravel app development kit for quick development.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MARVIN COLLINS",
            "email": "marvin@appslab.co.ke"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6"
    },
    "autoload": {
        "psr-4": {
            "LaravelDevkit\\" : "laravel-devkit/src/"
        },
        "files": [
            "laravel-devkit/src/Support/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelDevkit\\LaravelDevkitServiceProvider"
            ]
        }
    }
}